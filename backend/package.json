{
    "name": "@kf-products-core/kfhub_tarc_svc_nest",
    "private": true,
    "version": "11.0.0-140",
    "description": "KF Profile Manager Services",
    "main": "index.js",
    "engines": {
        "node": ">=22"
    },
    "engineStrict": true,
    "scripts": {
        "build": "bash ./scripts/build.sh",
        "build:docs": "node scripts/copyGeneratedAssets.js && npx compodoc -p tsconfig.app.json --unitTestCoverage ./documentation/coverage/coverage-summary.json --hideGenerator --name kfhub_tarc_svc_nest --silent",
        "ci-prebuild": "bash deploy/scripts/prebuild.sh",
        "ci-prebuild-prod": "bash deploy/scripts/prebuild-prod.sh",
        "ci-put-local": "node ./node_modules/@kf-products-core/kfhub_cicd/lib/local_put.js",
        "ci-put-s3": "node ./node_modules/@kf-products-core/kfhub_cicd/lib/s3_put.js",
        "configure": "bash ./scripts/configure.sh",
        "postbuild": "node scripts/copyAssets.js",
        "start:dev": "nodemon index.js",
        "start:release": "npm run build && npm run configure && cd release && node main.js",
        "test": "jest --config=jest.json --no-coverage --reporters=default --silent",
        "test:coverage": "jest --config=jest.json --coverage --silent",
        "test:watch": "jest --watch --config=jest.json --no-coverage",
        "tsc": "tsc -p tsconfig.app.json"
    },
