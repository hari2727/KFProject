    "name": "@kf-products-core/kfhub_tarc_lib",
    "version": "7.1.0-2048",
    "license": "LicenseRef-LICENSE",
    "scripts": {
        "analyze": "npm run build:no-sourcemap -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
        "build": "ng build --configuration=build --project kfhub_tarc_app",
        "build:all": "bash scripts/build_all.sh",
        "build:no-sourcemap": "npm run space:size && ng build --configuration=build,no-sourcemap --project kfhub_tarc_app",
        "ci-prebuild": "bash deploy/scripts/prebuild.sh",
        "ci-prebuild-prod": "bash deploy/scripts/prebuild-prod.sh",
        "ci-put-local": "node ./node_modules/@kf-products-core/kfhub_cicd/lib/local_put.js",
        "ci-put-s3": "node ./node_modules/@kf-products-core/kfhub_cicd/lib/s3_put.js",
        "deploy": "node ./node_modules/@kf-products-core/kfhub_cicd/lib/s3_put.js",
        "e2e": "ng e2e",
        "lint": "ng lint",
        "ng": "ng",
        "rebuild:lib": "bash scripts/rebuild_kfhub_lib.sh",
        "rebuild:tatmlib": "bash scripts/rebuild_kfhub_tatm_lib.sh",
        "rebuild:thclib": "bash scripts/rebuild_kfhub_thcl_lib.sh",
        "rebuild:thcllib": "bash scripts/rebuild_kfhub_thcl_lib.sh",
        "space:size": "cross-env NODE_OPTIONS=--max_old_space_size=8192",
        "start": "npm run space:size && ng cache disable && ng serve --proxy-config proxy.conf.json --project kfhub_tarc_app",
        "start:dev": "bash scripts/start_servers.sh",
        "start:dev-local": "ng serve --proxy-config proxy.conf.json --disable-host-check --project kfhub_tarc_app",
        "start:ll": "ng cache disable && ng serve --project tarc_ll --proxy-config proxy.conf.json",
        "sync": "cp ../kfhub_lib/kfhub-deploy-config.json ./ && cp -R ../kfhub_lib/scripts/ ./scripts",
        "test": "jest --verbose",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged --relative",
            "pre-push": [
                "lint-staged --relative"
            ]
        }
    },
