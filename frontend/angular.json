{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kfhub_tarc_app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "sourceMap": true,
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
                "includePaths": ["node_modules/"]
            },
            "assets": [
              "src/assets",
              "src/languages",
              "src/favicon.ico",
              {
                "glob": "build.txt",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "version.html",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@kf-products-core/kfhub_lib/kfhub_app",
                "output": "kfhub_app"
              }
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/dragula/dist/dragula.min.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css"
            ],
            "scripts": [
              "scripts/env.js",
              "scripts/mf_registry.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "build": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "no-sourcemap": {
              "sourceMap": false,
              "optimization": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kfhub_tarc_app:build",
            "port": 4300
          },
          "configurations": {
            "production": {
              "browserTarget": "kfhub_tarc_app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kfhub_tarc_app:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.js",
            "lintFilePatterns": [
              "src/app/**/*.ts",
              "src/app/**/*.html"
            ]
          }
        }
      }
    },
    "tarc_ll": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.ll.json",
            "sourceMap": true,
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
                "includePaths": ["node_modules/"]
            },
            "assets": [
              "src/assets",
              "src/languages",
              "src/favicon.ico",
              {
                "glob": "build.txt",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "version.html",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "./node_modules/dragula/dist/dragula.min.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tarc_ll:build",
            "port": 4300
          },
          "configurations": {
            "production": {
              "browserTarget": "tarc_ll:build:production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "kftarc"
    },
    "@schematics/angular:directive": {
      "prefix": "kftarc"
    }
  },
  "cli": {
    "cache": {
      "enabled": false
    },
    "analytics": "85a55a4c-bf01-46b9-b2b8-8e35c9236528"
  }
}
